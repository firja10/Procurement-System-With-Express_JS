<%- include('../layouts/header.ejs') %>
                
                <!-- Page-header start -->
                 <div class="page-header">
                    <div class="page-block">
                        <div class="row align-items-center">
                            <div class="col-md-8">
                                <div class="page-header-title">
                                    <h5 class="m-b-10">Dashboard</h5>
                                    <p class="m-b-0">Selamat Datang di PT. Perkakas Rekadaya Nusantara</p>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <ul class="breadcrumb-title">
                                    <li class="breadcrumb-item">
                                        <a href="index.html"> <i class="fa fa-home"></i> </a>
                                    </li>
                                    <li class="breadcrumb-item"><a href="#!">Dashboard</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Page-header end -->
                  <div class="pcoded-inner-content">
                      <!-- Main-body start -->
                      <div class="main-body">
                          <div class="page-wrapper">
                            
                            
                            
                            <!-- Page-body start -->
                              <div class="page-body">
                                  <div class="row">
                                     
                                    <% if(jabatan == 'sales') { %>
                                      <canvas id="myChartUnderBracket"></canvas>

                                      <canvas id="myChartInnerTube"></canvas>

                                      <canvas id="myChartTotal"></canvas>

                                      <% } else if(jabatan == 'produksi') { %>

                                        <canvas id="ProduksiUnderBracket"></canvas>
                                        <canvas id="ProduksiInnerTube"></canvas>


                                        <% } else if(jabatan == 'store') { %>


                                            <center>
                                                <h3>Tabel Bahan Baku</h3>
                                            </center>
                                 

                                            <table class="table" id="bahan_baku_tabel">
                                                <thead>
                                                 
                                                    <tr>
                                                    <th scope="col">No</th>
                                                    <th scope="col">Nama Bahan Baku</th>
                                                    <th scope="col">Nomor Part</th>
                                                    <th scope="col">Bahan Baku Masuk</th>
                                                    <th scope="col">Bahan Baku Keluar</th>
                                                    <th scope="col">Stok</th>
                                                  </tr>
                                                
                                                </thead>
                                                <tbody>

                                            <% if ( bahan_baku && bahan_baku.length){ %>

                                                <% for(var i = 0; i < bahan_baku.length; i++){ %>

                                                  <tr>
                                                    <th scope="row"><%= (i+1) %></th>
                                                    <td><%= bahan_baku[i].nama_bahan %></td>
                                                    <td><%= bahan_baku[i].no_part %></td>
                                
                                        
                                                    <td><%= bahan_baku[i].count_masuk %></td>
                                                    <td><%= bahan_baku[i].count_keluar %></td>
                                                    <td><%= bahan_baku[i].stok %></td>

                                                </tr>

                                                <% } %>

                                                <% } else { %>

                                                    <tr>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                    </tr>

                                                <% } %>
                                             
                                        
                                                </tbody>
                                              </table>


                                                <br>

                                                <center>
                                                    <h3>Tabel Produk Jadi</h3>
                                                </center>
                                                
                                              <table class="table" id="produk_jadi_tabel">
                                                <thead>
                                                 
                                                    <tr>
                                                    <!-- <th scope="col">No</th> -->
                                                    <th scope="col">Nama Produk Jadi</th>
                                                    <th scope="col">Nomor Part</th>
                                                    <th scope="col">Produk Jadi Masuk</th>
                                                    <th scope="col">Produk Jadi Keluar</th>
                                                    <th scope="col">Stok</th>
                                                  </tr>
                                                
                                                </thead>
                                                <tbody>

                                                    
                                                
                                                <% if ( produk_jadi && produk_jadi.length){ %>



                                                    <% for(var i = 0; i < produk_jadi.length; i++){ %>

                                                        <tr>
                                                          <th scope="row"><%= (i+1) %></th>
                                                          <td><%= produk_jadi[i].nama_bahan %></td>
                                                          <td><%= produk_jadi[i].no_part %></td>
                                      
                                              
                                                          <td><%= produk_jadi[i].count_masuk %></td>
                                                          <td><%= produk_jadi[i].count_keluar %></td>
                                                          <td><%= produk_jadi[i].stok %></td>
      
                                                      </tr>
      
                                                      <% } %>

                                                      <% } else { %>

                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>
                                                        <td></td>


                                                        <% } %>
                                             
                                        
                                                </tbody>
                                              </table>

    
                                        <% } else { %>

                                            <% } %>


                            


                                  </div>
                              </div>
                              <!-- Page-body end -->
                          </div>


                          <%- include('../layouts/footer.ejs') %>